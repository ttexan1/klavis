# Notion API Key (optional, can be provided via x-auth-token header)
NOTION_API_KEY=your_notion_api_key

# Notion API Version (optional, defaults to 2022-06-28)
NOTION_VERSION=2022-06-28

# Notion API Base URL (optional)
NOTION_API_BASE_URL=https://api.notion.com/v1

# Path to OpenAPI spec (optional)
# OPENAPI_SPEC_PATH=./scripts/notion-openapi.json

# Port to run the server on (optional, defaults to 5000)
PORT=5000 